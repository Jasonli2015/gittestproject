<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>gittestproject</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>ComboBoxServlet</servlet-name>
  	<servlet-class>com.jason.extjs.ComboBoxServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ComboBoxServlet</servlet-name>
  	<url-pattern>/ComboBoxServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>FileOperationServlet</servlet-name>
  	<servlet-class>com.jason.extjs.FileOperationServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>FileOperationServlet</servlet-name>
  	<url-pattern>/FileOperationServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>TreeServlet</servlet-name>
  	<servlet-class>com.jason.extjs.TreeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>TreeServlet</servlet-name>
  	<url-pattern>/TreeServlet</url-pattern>
  </servlet-mapping>
  
</web-app>